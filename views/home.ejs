<%- include("partials/header") -%>

<h1>Home</h1>
<p>
    <form action="/compose">
       <button class="btn btn-success comp" type="submit">Compose +</button>
    </form>

    <%= homeStartingContent %>

    <% posts.forEach(element => { %>
    <h2> <%= element.title%></h2>
    <p><%=  element.content.slice(0,25)+"..." %><a href="/posts/<%= element.title %>">Read more</a></p>
    <% }) %>

</p>
<%- include("partials/footer") -%>